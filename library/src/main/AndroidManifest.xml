<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.obsez.android.libs.kcxt">


    <application>
        <provider
            android:name="com.obsez.android.libs.kcxt.AppContextProvider"
            android:authorities="${applicationId}.providers.applicationprovider"
            android:directBootAware="true"
            android:exported="false"
            android:initOrder="999"
            android:multiprocess="true"
            tools:ignore="UnusedAttribute"/>
        <provider
            android:name="com.obsez.android.libs.kcxt.LastActivityProvider"
            android:authorities="${applicationId}.providers.lastactivityprovider"
            android:directBootAware="true"
            android:exported="false"
            android:initOrder="999"
            android:multiprocess="true"
            tools:ignore="UnusedAttribute"/>
    </application>


</manifest>
